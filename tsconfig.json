{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "rootDir": "./src",
    "outDir": "./dist",
	"moduleResolution": "bundler",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
	"experimentalDecorators": true,
	"emitDecoratorMetadata": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  },
  "exclude": [
	"node_modules"
  ]
}
